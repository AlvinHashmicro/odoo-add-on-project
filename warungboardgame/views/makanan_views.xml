<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- warung.makanan tree view -->
    <record id="warung_makanan_view_tree" model="ir.ui.view">
        <field name="name">Makanan</field>
        <field name="model">warung.makanan</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="name" />
                <field name="deskripsi" />
                <field name="harga" />
            </tree>
        </field>
    </record>

    <!-- warung.makanan form view -->
    <record id="warung_makanan_view_form" model="ir.ui.view">
        <field name="name">Form Makanan</field>
        <field name="model">warung.makanan</field>
        <field name="arch" type="xml">
            <form string="Daftar Makanan">
                <sheet>
                    <group>
                        <!-- Add your fields here -->
                        <field name="name" />
                        <field name="harga" />
                    </group>

                    <group>
                        <field name="deskripsi" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- warung.makanan action window -->
    <record id="warung_makanan_action" model="ir.actions.act_window">
        <field name="name">Makanan</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">warung.makanan</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- This Menu Item must have a parent and an action -->
    <menuitem
        id="warung_makanan_menu_act"
        name="Makanan"
        parent="warung_fnb_menu_categ"
        action="warung_makanan_action"
        sequence="30"/>
</odoo>
